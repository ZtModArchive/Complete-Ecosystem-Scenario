<ZTScenarioMgr interval="2" name="mgr" state="0" chain="true" allsuccess="true" sticky="true" optional="false">
	<info locid="TheWorld:HugeBiomeintro" />
	<children>
		<BFScenarioRule hidden="1" name="setinitialzoostate" state="0" sticky="true">
			<evaluate>
				<BFScenarioScriptAction script="scenario/scripts/worldcampaign.lua" entry="setinitialzoostate" />
			</evaluate>
		</BFScenarioRule>
		<BFScenarioGroup hidden="0" name="hugebiome" state="0" chain="true" allsuccess="true" sticky="true">
			<info type="primary">
				<neutral overview="TheWorld:HugeBiomeoverview" />
				<success locid="TheWorld:HugeBiomeresult" overview="HugeBiomeSuccessoverview" />
				<failure locid="TheWorld:HugeBiomeFailureoverview" />
			</info>
			<success>
				<BFScenarioScriptAction script="scenario/scripts/worldcampaign_new.lua" entry="completeworldcampaignscen4" />
			</success>
			<children>
				<BFScenarioRule name="HugeBiomesetup" hidden="0" state="0" sticky="true">
					<info type="primary" locid="TheWorld:HugeBiomeoverallgoal">
						<neutral locid="TheWorld:HugeBiomeHabitatSetup" />
						<success locid="TheWorld:HugeBiomeHabitatSetupOK" />
						<failure locid="TheWorld:HugeBiomeoverallgoalfailed" overview="HugeBiomeFailureoverview" />
					</info>
					<evaluate>
						<BFScenarioScriptAction script="scenario/scripts/worldcampaign_new.lua" entry="evalhabitatsetup" />
					</evaluate>
					<success>
						<BFScenarioScriptAction script="scenario/scripts/ui.lua" entry="showprimarygoals" />
					</success>
					<failure>
						<BFScenarioScriptAction script="scenario/scripts/worldcampaign_new.lua" entry="failworldcampaignscen4" />
					</failure>
				</BFScenarioRule>			
				<BFScenarioRule name="HugeBiomeoverall" hidden="1" state="0" sticky="true">
					<info type="primary" locid="TheWorld:HugeBiomeoverallgoal">
						<neutral locid="TheWorld:HugeBiomeoverallgoal" />
						<success locid="TheWorld:HugeBiomeoverallgoalOK" />
						<failure locid="TheWorld:HugeBiomeoverallgoalfailed" overview="HugeBiomeFailureoverview" />
					</info>
					<evaluate>
						<BFScenarioScriptAction script="scenario/scripts/worldcampaign_new.lua" entry="evalhugebiome" />
					</evaluate>
					<success>
						<BFScenarioScriptAction script="scenario/scripts/ui.lua" entry="showprimarygoals" />
					</success>
					<failure>
						<BFScenarioScriptAction script="scenario/scripts/worldcampaign_new.lua" entry="failworldcampaignscen4" />
					</failure>
				</BFScenarioRule>
			</children>
		</BFScenarioGroup>
	</children>
</ZTScenarioMgr>
